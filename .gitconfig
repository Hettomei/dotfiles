[gui]
[user]
  name = Hettomei
  email = itsumo.sibyllin@gmail.com
[core]
  editor = vim
  excludesfile = /Users/tim/.gitignore_global
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[alias]
  st = status
  br = branch
  co = checkout
