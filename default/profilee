if [ -n "$TMUX" ];
then
  # Inside tmux, we don't want to reload this file
  return 0
fi

if [ -d "/home/linuxbrew/.linuxbrew/bin" ] ; then
    eval $(/home/linuxbrew/.linuxbrew/bin/brew shellenv)

    [[ -r "/home/linuxbrew/.linuxbrew/etc/profile.d/bash_completion.sh" ]] && . "/home/linuxbrew/.linuxbrew/etc/profile.d/bash_completion.sh"

    # for COMPLETION in "${HOMEBREW_PREFIX}/etc/bash_completion.d/"*; do
    #   [[ -r "$COMPLETION" ]] && source "$COMPLETION"
    # done
fi

# already package by android studio
if [ -d "$HOME/programmes/android-studio/jre" ] ; then
  export JAVA_HOME="$HOME/programmes/android-studio/jre";
  PATH=$PATH:$JAVA_HOME/bin
fi

if [ -d "$HOME/programmes/Android" ] ; then
  export ANDROID_HOME=$HOME/programmes/Android
  PATH=$PATH:$ANDROID_HOME/emulator
  PATH=$PATH:$ANDROID_HOME/tools
  PATH=$PATH:$ANDROID_HOME/tools/bin
  PATH=$PATH:$ANDROID_HOME/platform-tools
fi

if [ "$(command -v yarn)" ]
then
  PATH="$(yarn global bin):$PATH"
fi
