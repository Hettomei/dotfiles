    [[ -r "/home/linuxbrew/.linuxbrew/etc/profile.d/bash_completion.sh" ]] && . "/home/linuxbrew/.linuxbrew/etc/profile.d/bash_completion.sh"

    # for COMPLETION in "${HOMEBREW_PREFIX}/etc/bash_completion.d/"*; do
