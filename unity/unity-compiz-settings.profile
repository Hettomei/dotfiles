s0_outputs = 1920x1080+1920+1080;1920x1080+0+0;
s0_autoraise = false
s0_autoraise_delay = 500
s0_maximize_window_key = <Control><Alt>f
s0_unmaximize_or_minimize_window_key = <Control><Alt>g

[shift]
