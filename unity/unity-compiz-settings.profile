[cubeaddon]
s0_ground_color1 = #b3b3b3cc
s0_ground_color2 = #b3b3b300

[showmouse]
s0_color = #ffdf3fff

[unityshell]
s0_lock_screen = Disabled
s0_show_hud = Disabled
s0_background_color = #ffffff00
s0_dash_blur_experimental = 0
s0_low_graphics_mode = true
s0_inactive_shadow_color = #000000a5
s0_show_launcher = Disabled
s0_launcher_opacity = 0,800000
s0_icon_size = 50
s0_alt_tab_forward = Disabled
s0_alt_tab_prev = Disabled
s0_alt_tab_forward_all = Disabled
s0_alt_tab_prev_all = Disabled
s0_show_minimized_windows = false
s0_alt_tab_timeout = false
s0_alt_tab_bias_viewport = false

[animation]
s0_unminimize_effects = animation:Glide 2;

[firepaint]
s0_fire_color = #ff3305ff

[put]
